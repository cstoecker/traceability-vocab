{
    	"$id": "https://w3id.org/traceability/schemas/HTScodeEvidenceCredential.json",
    	"$schema": "http://json-schema.org/draft-07/schema#",
    	"$comment": "{\"term\": \"HTScodeEvidenceCredential\", \"@id\": \"https://w3id.org/traceability/HTScodeEvidenceCredential\"}",
    	"title": "HTScodeEvidenceCredential",
    	"description": "Reference to HTS Codes Version and URL",
	"type": "object",
	"required": [
		"Country",
		"Version",
		"VersionURL"
	],
	"properties": {
        	"@context": {
            		"type": "array"
        	},
        	"type": {
            		"oneOf": [
                		{
                    			"type": "string"
                		},
                		{
                    			"type": "array"
                		}
            		]
        	},
		"Country": {
			"$comment": "{\"term\": \"Country\", \"@id\": \"https://schema.org/Country\"}",
			"title": "Issuing Country of the HTS Code Definitions - ISO 3166-1 alpha-2 country code", 
			"type": "string",
			"default": "US"
		},
		"Version": {
			"$comment": "{\"term\": \"Version\", \"@id\": \"https://schema.org/version\"}",
			"title": "Version", 
			"type": "string",
			"examples": [
				"2020 Revision 28"
			]
		},
		"VersionURL": {
			"$comment": "{\"term\": \"VersionURL\", \"@id\": \"https://schema.org/url\"}", 
			"title": "VersionURL", 
			"type": "string",
			"examples": [
				"https://hts.usitc.gov/view/release?release=2020HTSARev28"
			]
		}
	}
}









